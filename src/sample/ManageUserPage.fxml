<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="485.0" prefWidth="834.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ManageUserController">
   <children>
      <Label layoutX="342.0" layoutY="36.0" text="Manage User">
         <font>
            <Font name="Arial Black" size="21.0" />
         </font>
      </Label>
      <TableView fx:id="cashierTable" layoutX="39.0" layoutY="150.0" prefHeight="270.0" prefWidth="476.0">
        <columns>
          <TableColumn fx:id="cashierIDCol" prefWidth="75.0" text="User ID" />
          <TableColumn fx:id="cashierNameCol" prefWidth="170.0" text="Username" />
            <TableColumn fx:id="cashierPassCol" prefWidth="114.0" text="Password" />
            <TableColumn fx:id="adminStatusCol" prefWidth="116.0" text="Admin Status" />
        </columns>
      </TableView>
      <Button layoutX="585.0" layoutY="150.0" mnemonicParsing="false" onAction="#newCashierButtonClicked" prefHeight="56.0" prefWidth="160.0" text="New User" />
      <Button layoutX="585.0" layoutY="243.0" mnemonicParsing="false" onAction="#editCashierButtonClicked" prefHeight="56.0" prefWidth="160.0" text="Edit User" />
      <Button layoutX="585.0" layoutY="340.0" mnemonicParsing="false" onAction="#deleteCashierButtonClicked" prefHeight="56.0" prefWidth="160.0" text="Delete User" />
      <Button layoutX="25.0" layoutY="26.0" mnemonicParsing="false" onAction="#homeButtonClicked" text="HOME" />
   </children>
</AnchorPane>
